<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" android:background="#ffffff">
    <FrameLayout android:id="@+id/rssBackground" android:layout_width="fill_parent" android:background="#eeeeee" android:layout_height="wrap_content">
    	<LinearLayout android:layout_width="fill_parent" android:id="@+id/linearLayout1" android:layout_height="wrap_content" android:orientation="horizontal">
    		<FrameLayout android:paddingLeft="9dp" android:paddingRight="4dp" android:paddingTop="10dp" android:paddingBottom="10dp" android:id="@+id/frameLayout2" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_gravity="center_horizontal">
    			<FrameLayout android:id="@+id/frame" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_gravity="center">
    		    </FrameLayout>
    		</FrameLayout>
    	<LinearLayout android:orientation="vertical"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:layout_weight="1">
            <TextView android:layout_height="wrap_content" android:text="TextView" 
            	android:id="@+id/rssRowText1" android:layout_gravity="top" 
            	android:typeface="serif" android:layout_width="fill_parent" 
            	android:paddingTop="5dp" android:paddingBottom="5dp" 
            	android:textSize="17sp" android:paddingLeft="5dp" 
            	android:textColor="#000000"></TextView>
    	
    		<TextView android:layout_height="wrap_content" android:text="TextView" 
    			  android:layout_below="@+id/rssRowText1"
            	android:id="@+id/rssRowText2" android:layout_gravity="top" 
            	android:typeface="normal" android:layout_width="fill_parent" 
            	android:paddingTop="5dp" android:paddingBottom="5dp" 
            	android:textSize="13sp" android:paddingLeft="20dp" 
            	android:textColor="@color/grey"></TextView>
            	</LinearLayout>
            	</LinearLayout>
    </FrameLayout>
</LinearLayout>